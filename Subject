package observerPattern;

 /**This class is an Interface which is used in both the StockGrabber class and the StockDownloader class to add 
 * stock market prices to be stored in the ArrayList and to delete stock market prices from th ArrayList.
  * @author   Elim Yao Tsiagbey
 * @version   1.0.0 9/16/2015
 */
public interface Subject {
	
	public void register(Observer newObserver);
	public void unregister(Observer deleteObserver);
	public void notifyObserver();
}
